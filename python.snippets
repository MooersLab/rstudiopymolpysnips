	cmd.do("iterate (resi ${1:133}), Bfactors.append(b);")
	cmd.do("print( \"Average B-factor of residue \", %{1:133} , \"   = \", \"%.2f\" %(reduce(lambda x, y: x + y, Bfactors) / float(len(Bfactors))) );")
