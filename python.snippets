# Save an aln flle with a timestamp.
snippet saln
	cmd.do("python;")
	cmd.do("import datetime;")
	cmd.do("from pymol import cmd;")
	cmd.do("DT =datetime.datetime.now().strftime(\"yr%Ymo%mday%dhr%Hmin%M\");")
	cmd.do("s = str(DT);")
	cmd.do("cmd.save(stemName+s+\".aln\");")
	cmd.do("python end;")
	${0}
